{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "experimentalDecorators": true,
    "noImplicitOverride": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist/"
  },
  "include": ["src/**/*"],
  "exclude": ["dist", "node_modules"]
}
